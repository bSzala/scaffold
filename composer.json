{
    "name": "fenix440/scaffold",
    "description": "Scaffolding utility tool for generating various templates/resources via a CLI. The goal is to speed up a package/project development",
    "keywords": [
        "scaffold",
        "template",
        "resource",
        "utility"
    ],
    "homepage": "https://github.com/fenix440/scaffold",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bartlomiej Szala",
            "email": "fenix440@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Fenix440\\Scaffold\\": "src/"
        }
    },
    "require": {
        "symfony/console": "^2.7",
        "symfony/finder": "^2.7",
        "symfony/filesystem": "^2.7"
    },
    "bin": ["scaffold"],
    "minimum-stability": "stable"
}
