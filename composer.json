{
    "name": "bszala/scaffold",
    "description": "Scaffolding utility tool for generating various templates/resources via a CLI. The goal is to speed up a package/project development",
    "keywords": [
        "scaffold",
        "template",
        "resource",
        "utility"
    ],
    "homepage": "https://github.com/bszala/scaffold",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Bartlomiej Szala",
            "email": "fenix440@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "BSzala\\Scaffold\\": "src/"
        }
    },
    "require": {
        "php": ">=5.5.9",
        "symfony/finder": "^3.2",
        "symfony/filesystem": "^3.2",
        "symfony/console": "^3.2"
    },
    "require-dev": {
        "codeception/codeception": "^2.2",
        "mockery/mockery": "^0.9.4"
    },
    "bin": [
        "scaffold"
    ],
    "minimum-stability": "stable"
}
